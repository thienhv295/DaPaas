@charset "utf-8";

/* ------------------------------------------------------------
 * Name      : default.css
 * Created   : 2015/09/25 CHJ
 ------------------------------------------------------------ */

* {margin:0; padding:0;}
html {width:100%; height:100%; overflow-y:auto;}
body {width:100%; height:100%; color:#333; font-size:12px; font-family:NanumGothic,나눔고딕,'Nanum Gothic','맑은 고딕',HelveticaNeue,DroidSans,Sans-serif,HelveticaDotum; line-height:18px;}
h1,h2,h3,h4,h5,h6 {font-size:12px; text-align:left; font-weight:600;}
img,fieldset,iframe {border:0 none; vertical-align:top;}
ol,ul,li {list-style:none;}
em,address {font-style:normal;}
hr, legend, caption {display:none;}
input, select, textarea {vertical-align:middle;}
em,address{font-style:normal;}
label {cursor:pointer; vertical-align:middle; outline-width:0;}
table {width:100%; table-layout:fixed; border-collapse:collapse;}

select, input, textarea {font-family:NanumGothic,나눔고딕,'Nanum Gothic','맑은 고딕',HelveticaNeue,DroidSans,Sans-serif,HelveticaDotum; color:#333; font-size:12px;}

ul, li {*zoom:1;}
ul:after, li:after {content:""; clear:both; display:block;}

/* 웹폰트 CSS */
@font-face{font-family:'Nanum Gothic'; src:url(/webfont/NanumGothic.eot);}
@font-face{font-family:'Nanum Gothic'; src:url(/webfont/NanumGothic.woff);}

body {background:#f7f7f9;}
.wrap {height:100%; position:relative;}

#header {display:block; overflow:hidden; width:100%; height:70px; position:absolute; top:0px; left:0px; border-bottom:1px solid #ddd; background:#fff;}
#header h1 {float:left; font-size:23px; font-weight:700; color:#337ab7; margin:23px 0 0 20px;}

.menu_area {float:right; margin:17px 20px 0 0;}
.menu_area li {float:left;}

#content {display:block;  width:100%; height:100%;}
.con_left {float:left; display:inline-block; width:50%; min-height:100%; padding:85px 15px 15px; background:url(../images/bg_line.gif) right 0 repeat-y;}
.con_right {float:right; display:inline-block; width:50%; height:100%; padding:85px 15px 15px;}

.box01 {display:block;}
.box01 ul {}
.box01 ul li {float:left; margin-right:10px; margin-bottom:10px;}
.box01 ul li:last-child {margin-right:0px;}
.box02 {display:block;} 
.box02 .form-control {width:100%;}

.box03 {display:block; margin-top:30px;}

.panel1 {height:98%;}
.panel2 {height:48.3%;}
.panel-heading {font-size:18px;}
.panel-body {}
.col-sm-9 {position:relative;}
.btn_close {position:absolute; top:6px; right:20px; width:18px; height:18px; display:block; overflow:hidden; cursor:pointer; background:url(../images/btn_close.gif) 0 0 no-repeat; line-height:999px;}

.b2_ip {width:86%;}
.alert-info {padding-bottom:0px;}
.alert-default {background:#f7f7f9; border:1px solid #dddddd; padding-bottom:0px;}
.btn_on {background:#337ab7; border:1px solid #2e6da4; color:#fff;}